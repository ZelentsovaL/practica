<Window x:Class="practice.Views.UpdateEmployee"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practice.Views"
        mc:Ignorable="d"
        Title="UpdateEmployee" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Width="150">
            <TextBox x:Name="LastNameBox"/>
            <TextBox x:Name="FirstNameBox"/>
            <TextBox x:Name="MiddleNameBox"/>
            <TextBox x:Name="PositionIdBox"/>
            <TextBox x:Name="DepartmentIdBox"/>
            <TextBox x:Name="InfoBox"/>
            <Button x:Name="UpdEmployee" Content="Update" Click="UpdEmployee_Click"/>
        </StackPanel>
    </Grid>
</Window>
